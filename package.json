{
  "name": "vite-plugin-static-filehash",
  "version": "0.0.5",
  "description": "A Vite plugin that tries to keep the hash of a single file unchanged",
  "type": "module",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "build:tsc": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "url": "https://github.com/MrQinYQ/vite-plugin-static-filehash.git"
  },
  "keywords": [
    "vite",
    "hash",
    "file",
    "vite-plugin"
  ],
  "author": "qinyequan",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node12": "^12.1.3",
    "@types/node": "^22.1.0",
    "rollup": "^4.20.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "es-module-shims": "^1.10.0",
    "magic-string": "^0.30.11"
  }
}
